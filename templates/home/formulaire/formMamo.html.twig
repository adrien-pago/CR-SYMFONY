{% extends 'base.html.twig' %}

{% block title %}Mammographie - CR-SYMFONY{% endblock %}

{% block header %}
    {{ include('partials/_header.html.twig') }}
{% endblock %}

{% block body %}
<div class="container-fluid mt-4">
    <div class="row">
        <div class="col-12">
            <div class="card main-card">
                <div class="card-header d-flex justify-content-between align-items-center">
                    <h2 class="card-title mb-0">Compte-rendu de Mammographie</h2>
                    <a href="{{ path('app_home') }}" class="btn btn-outline-secondary">
                        <i class="bi bi-arrow-left"></i> Retour
                    </a>
                </div>
                <div class="card-body">
                    {{ form_start(form, {'attr': {'class': 'needs-validation'}}) }}
                        {# Section Titre #}
                        <div class="card mb-4">
                            <div class="card-header text-center d-flex justify-content-between align-items-center">
                                <h4 class="mb-0">Titre</h4>
                                <i class="bi bi-eye toggle-visibility" data-target="titre-content"></i>
                            </div>
                            <div class="card-body titre-content">
                                {{ form_widget(form.titre) }}
                            </div>
                        </div>

                        {# Section Indication #}
                        <div class="card mb-4">
                            <div class="card-header text-center d-flex justify-content-between align-items-center">
                                <h4 class="mb-0">Indication</h4>
                                <i class="bi bi-eye toggle-visibility" data-target="indication-content"></i>
                            </div>
                            <div class="card-body indication-content">
                                <div class="mb-4">
                                    {{ form_widget(form.indication) }}
                                </div>

                                {# Partie Bilan de la section Indication #}
                                <div class="mb-4">
                                    <button type="button" id="bilanBtn" class="btn btn-outline-primary d-flex align-items-center">
                                        <i class="bi bi-plus-circle me-2"></i> <span class="bilan-text">Bilan</span>
                                    </button>
                                    
                                    {# Options du bilan #}
                                    <div id="bilanOptions" class="d-none mt-3">
                                        <div class="d-flex gap-2 mb-4 flex-wrap">
                                            <button type="button" class="btn btn-outline-secondary option-btn" data-target="douleurOptions">
                                                <i class="bi bi-dot-circle me-2"></i>Douleur
                                            </button>
                                            <button type="button" class="btn btn-outline-secondary option-btn" data-target="masseOptions">
                                                <i class="bi bi-dot-circle me-2"></i>Masse
                                            </button>
                                            <button type="button" class="btn btn-outline-secondary option-btn" data-target="ecoulementOptions">
                                                <i class="bi bi-dot-circle me-2"></i>Écoulement
                                            </button>
                                            <button type="button" class="btn btn-outline-secondary option-btn" data-target="gynecomastieOptions">
                                                <i class="bi bi-dot-circle me-2"></i>Gynécomastie
                                            </button>
                                            <button type="button" class="btn btn-outline-secondary option-btn" data-target="autreOptions">
                                                <i class="bi bi-dot-circle me-2"></i>Autre
                                            </button>
                                            <button type="button" id="resetBtn" class="btn btn-outline-danger ms-auto">
                                                <i class="bi bi-x-circle me-2"></i>Reset
                                            </button>
                                        </div>

                                        {# Options pour la douleur #}
                                        <div id="douleurOptions" class="option-container d-none mt-3">
                                            <p class="text-muted mb-2">d'une douleur mammaire</p>
                                            <div class="row">
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="douleur_lateralite">
                                                        <option value="">Latéralité ?</option>
                                                        <option value="gauche">Gauche</option>
                                                        <option value="droite">Droite</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="douleur_apparition">
                                                        <option value="">Apparition</option>
                                                        <option value="spontanee">Spontanée</option>
                                                        <option value="post_traumatique">Post-traumatique</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="douleur_anciennete">
                                                        <option value="">Ancienneté</option>
                                                        <option value="recente">Récente</option>
                                                        <option value="ancienne">Datant de plusieurs mois</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        {# Options pour la masse #}
                                        <div id="masseOptions" class="option-container d-none mt-3">
                                            <p class="text-muted mb-2">d'une masse mammaire</p>
                                            <div class="row">
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="masse_lateralite">
                                                        <option value="">Latéralité ?</option>
                                                        <option value="gauche">Gauche</option>
                                                        <option value="droite">Droite</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="masse_palpation">
                                                        <option value="">Palpation</option>
                                                        <option value="recente">De découverte récente</option>
                                                        <option value="ancienne_stable">Ancienne, stable</option>
                                                        <option value="ancienne_evolutive">Ancienne, évolutive</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        {# Options pour l'écoulement #}
                                        <div id="ecoulementOptions" class="option-container d-none mt-3">
                                            <p class="text-muted mb-2">d'un écoulement mammelonnaire</p>
                                            <div class="row">
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="ecoulement_lateralite">
                                                        <option value="">Latéralité ?</option>
                                                        <option value="gauche">Gauche</option>
                                                        <option value="droite">Droite</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="ecoulement_type">
                                                        <option value="">Type</option>
                                                        <option value="unipore">Unipore</option>
                                                        <option value="multipore">Multipore</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="ecoulement_aspect">
                                                        <option value="">Aspect</option>
                                                        <option value="clair">Clair</option>
                                                        <option value="lactescent">Lactescent</option>
                                                        <option value="verdatre">Verdâtre</option>
                                                        <option value="sanguinolant">Sanguinolant</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        {# Options pour la gynécomastie #}
                                        <div id="gynecomastieOptions" class="option-container d-none mt-3">
                                            <p class="text-muted mb-2">d'une gynécomastie</p>
                                            <div class="row">
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="gynecomastie_lateralite">
                                                        <option value="">Latéralité</option>
                                                        <option value="gauche">Gauche</option>
                                                        <option value="droite">Droite</option>
                                                        <option value="bilaterale">Bilatérale</option>
                                                    </select>
                                                </div>
                                                <div class="col-md-4 mb-2">
                                                    <select class="form-select" name="gynecomastie_douleur">
                                                        <option value="">Douleur</option>
                                                        <option value="douloureuse">Douloureuse</option>
                                                        <option value="indolore">Indolore</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        {# Option pour autre #}
                                        <div id="autreOptions" class="option-container d-none mt-3">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <textarea class="form-control" name="autre_description" rows="3" placeholder="Description de l'anomalie"></textarea>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            
                                {# Section Antécédents personnels #}
                                <div class="mb-4 ms-3">
                                    <h5 class="mb-3">Antécédents personnels sénologiques et ovariens</h5>
                                    <div class="d-flex gap-2 mb-3">
                                        <button type="button" class="btn btn-outline-secondary antecedents-btn" data-value="non">
                                            Non
                                        </button>
                                        <button type="button" class="btn btn-outline-secondary antecedents-btn" data-value="oui">
                                            Oui
                                        </button>
                                        <button type="button" class="btn btn-outline-secondary antecedents-btn" data-value="ne_sait_pas">
                                            Ne sait pas
                                        </button>
                                        <button type="button" id="resetAntecedentsBtn" class="btn btn-outline-danger">
                                            <i class="bi bi-x-circle"></i> Reset
                                        </button>
                                    </div>

                                    {# Conteneur pour les options détaillées #}
                                    <div id="antecedentsDetails" class="d-none mt-3">
                                        {# Cancer du sein #}
                                        <div class="card mb-3">
                                            <div class="card-header">
                                                <h5 class="mb-0">Cancer du sein</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row mb-3">
                                                    <div class="col-md-4">
                                                        <select class="form-select" name="cancer_sein" id="cancerSeinSelect">
                                                            <option value="">Non renseigné</option>
                                                            <option value="oui">Oui</option>
                                                            <option value="non">Non</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                {# Options si cancer du sein = oui #}
                                                <div id="cancerSeinDetails" class="d-none">
                                                    <div class="row mb-3">
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" name="cancer_sein_annee" placeholder="Année">
                                                        </div>
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="cancer_sein_histologie">
                                                                <option value="">Histologie</option>
                                                                <option value="carcinome_infiltrant">Carcinome infiltrant non spécifique</option>
                                                                <option value="carcinome_lobulaire">Carcinome lobulaire infiltrant</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="cancer_sein_traitement" id="cancerSeinTraitement">
                                                                <option value="">Traitement</option>
                                                                <option value="conservateur">Traitement conservateur</option>
                                                                <option value="mastectomie">Mastectomie totale</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    {# Options si mastectomie #}
                                                    <div id="mastectomieDetails" class="d-none">
                                                        <div class="row">
                                                            <div class="col-md-4">
                                                                <select class="form-select" name="mastectomie_reconstruction">
                                                                    <option value="">Reconstruction</option>
                                                                    <option value="sans">Sans geste de reconstruction</option>
                                                                    <option value="prothese">Reconstruction par prothèse</option>
                                                                    <option value="lipofilling">Reconstruction par lipofilling</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        {# Chirurgie pour lésion à risque #}
                                        <div class="card mb-3">
                                            <div class="card-header">
                                                <h5 class="mb-0">Chirurgie pour lésion à risque</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row mb-3">
                                                    <div class="col-md-4">
                                                        <select class="form-select" name="chirurgie_risque" id="chirurgieRisqueSelect">
                                                            <option value="">Non renseigné</option>
                                                            <option value="oui">Oui</option>
                                                            <option value="non">Non</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                {# Options si chirurgie à risque = oui #}
                                                <div id="chirurgieRisqueDetails" class="d-none">
                                                    <div class="row mb-3">
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="chirurgie_risque_type">
                                                                <option value="">Type de lésion</option>
                                                                <option value="hyperplasie_canalaire">Hyperplasie canalaire atypique</option>
                                                                <option value="hyperplasie_lobulaire">Hyperplasie lobulaire atypique</option>
                                                                <option value="carcinome_lobulaire">Carcinome lobulaire in situ</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="chirurgie_risque_lateralite">
                                                                <option value="">Latéralité ?</option>
                                                                <option value="gauche">Gauche</option>
                                                                <option value="droite">Droite</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" name="chirurgie_risque_annee" placeholder="Année">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        {# Chirurgie pour lésion bénigne #}
                                        <div class="card mb-3">
                                            <div class="card-header">
                                                <h5 class="mb-0">Chirurgie pour lésion bénigne</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row mb-3">
                                                    <div class="col-md-4">
                                                        <select class="form-select" name="chirurgie_benigne" id="chirurgieBenigneSelect">
                                                            <option value="">Non renseigné</option>
                                                            <option value="oui">Oui</option>
                                                            <option value="non">Non</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                {# Options si chirurgie bénigne = oui #}
                                                <div id="chirurgieBenigneDetails" class="d-none">
                                                    <div class="row mb-3">
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="chirurgie_benigne_type">
                                                                <option value="">Type de lésion</option>
                                                                <option value="fibroadenome">Fibroadénome</option>
                                                                <option value="papillome">Papillome intracanalaire</option>
                                                                <option value="tumeur_phyllode">Tumeur phyllode</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="chirurgie_benigne_lateralite">
                                                                <option value="">Latéralité ?</option>
                                                                <option value="gauche">Gauche</option>
                                                                <option value="droite">Droite</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" name="chirurgie_benigne_annee" placeholder="Année">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        {# Chirurgie plastique #}
                                        <div class="card mb-3">
                                            <div class="card-header">
                                                <h5 class="mb-0">Chirurgie plastique</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row mb-3">
                                                    <div class="col-md-4">
                                                        <select class="form-select" name="chirurgie_plastique" id="chirurgiePlastiqueSelect">
                                                            <option value="">Non renseigné</option>
                                                            <option value="oui">Oui</option>
                                                            <option value="non">Non</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                {# Options si chirurgie plastique = oui #}
                                                <div id="chirurgiePlastiqueDetails" class="d-none">
                                                    <div class="row mb-3">
                                                        <div class="col-md-4">
                                                            <select class="form-select" name="chirurgie_plastique_type" id="chirurgiePlastiqueType">
                                                                <option value="">Type de chirurgie</option>
                                                                <option value="reduction">Réduction mammaire</option>
                                                                <option value="prothese">Prothèse mammaire</option>
                                                                <option value="symetrisation">Symétrisation</option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    {# Options pour réduction mammaire #}
                                                    <div id="reductionDetails" class="d-none">
                                                        <div class="row mb-3">
                                                            <div class="col-md-4">
                                                                <select class="form-select" name="reduction_lateralite">
                                                                    <option value="">Latéralité</option>
                                                                    <option value="gauche">Gauche</option>
                                                                    <option value="droite">Droite</option>
                                                                    <option value="bilaterale">Bilatérale</option>
                                                                </select>
                                                            </div>
                                                            <div class="col-md-4">
                                                                <input type="number" class="form-control" name="reduction_annee" placeholder="Année">
                                                            </div>
                                                        </div>
                                                    </div>

                                                    {# Options pour prothèse mammaire #}
                                                    <div id="protheseDetails" class="d-none">
                                                        <div class="row mb-3">
                                                            <div class="col-md-4">
                                                                <select class="form-select" name="prothese_lateralite" id="protheseLateralite">
                                                                    <option value="">Latéralité</option>
                                                                    <option value="gauche">Gauche</option>
                                                                    <option value="droite">Droite</option>
                                                                    <option value="bilaterale">Bilatérale</option>
                                                                </select>
                                                            </div>
                                                        </div>

                                                        {# Options selon la latéralité #}
                                                        <div id="protheseGaucheDetails" class="d-none">
                                                            <div class="row mb-3">
                                                                <div class="col-md-4">
                                                                    <input type="number" class="form-control" name="prothese_gauche_annee" placeholder="Année">
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div id="protheseDroiteDetails" class="d-none">
                                                            <div class="row mb-3">
                                                                <div class="col-md-4">
                                                                    <input type="number" class="form-control" name="prothese_droite_annee" placeholder="Année">
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div id="protheseBilateraleDetails" class="d-none">
                                                            <div class="row mb-3">
                                                                <div class="col-md-6">
                                                                    <input type="number" class="form-control" name="prothese_bilaterale_gauche_annee" placeholder="Année à gauche">
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <input type="number" class="form-control" name="prothese_bilaterale_droite_annee" placeholder="Année à droite">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    {# Options pour symétrisation #}
                                                    <div id="symetrisationDetails" class="d-none">
                                                        <div class="row mb-3">
                                                            <div class="col-md-4">
                                                                <select class="form-select" name="symetrisation_lateralite">
                                                                    <option value="">Latéralité</option>
                                                                    <option value="gauche">À gauche</option>
                                                                    <option value="droite">À droite</option>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        {# Antécédent de néoplasie ovarienne #}
                                        <div class="card mb-3">
                                            <div class="card-header">
                                                <h5 class="mb-0">Antécédent de néoplasie ovarienne</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row mb-3">
                                                    <div class="col-md-4">
                                                        <select class="form-select" name="neoplasie_ovarienne" id="neoplasieOvarienneSelect">
                                                            <option value="">Non renseigné</option>
                                                            <option value="oui">Oui</option>
                                                            <option value="non">Non</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                {# Options si néoplasie ovarienne = oui #}
                                                <div id="neoplasieOvarienneDetails" class="d-none">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" name="neoplasie_ovarienne_annee" placeholder="Année">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        {# Maladie de Hodgkin #}
                                        <div class="card mb-3">
                                            <div class="card-header">
                                                <h5 class="mb-0">Maladie de Hodgkin avec irradiation thoracique</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <select class="form-select" name="hodgkin">
                                                            <option value="">Non renseigné</option>
                                                            <option value="oui">Oui</option>
                                                            <option value="non">Non</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                {# Section Âge de la patiente #}
                                <div class="mb-4 ms-3">
                                    <h5 class="mb-3">Âge de la patiente</h5>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <input type="number" class="form-control" name="age_patiente" placeholder="Âge en années">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                {# Section Statut hormonal #}
                                <div class="mb-4 ms-3">
                                    <h5 class="mb-3">Statut hormonal</h5>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <select class="form-select" name="statut_hormonal" id="statutHormonalSelect">
                                                <option value="">Statut hormonal</option>
                                                <option value="menopausee">Ménopausée</option>
                                                <option value="non_menopausee">Non ménopausée</option>
                                                <option value="menopause_en_cours">Ménopause en cours</option>
                                            </select>
                                        </div>
                                    </div>

                                    {# Options si ménopausée #}
                                    <div id="menopauseeDetails" class="d-none mt-3">
                                        <div class="row mb-3">
                                            <div class="col-md-4">
                                                <div class="input-group">
                                                    <span class="input-group-text">À l'âge de</span>
                                                    <input type="number" class="form-control" name="age_menopause" placeholder="Âge">
                                                    <span class="input-group-text">ans</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <select class="form-select" name="traitement_hormonal">
                                                    <option value="">Traitement hormonal substitutif</option>
                                                    <option value="pas_de_traitement">Pas de traitement hormonal substitutif</option>
                                                    <option value="en_cours">Traitement hormonal substitutif en cours</option>
                                                    <option value="arrete">Traitement hormonal substitutif arrêté</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    {# Options si non ménopausée #}
                                    <div id="nonMenopauseeDetails" class="d-none mt-3">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <select class="form-select" name="cycle">
                                                    <option value="">Cycle</option>
                                                    <option value="premiere_quinzaine">En première quinzaine du cycle</option>
                                                    <option value="seconde_quinzaine">En seconde quinzaine du cycle</option>
                                                    <option value="milieu_cycle">En milieu de cycle</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                {# Section Enfants #}
                                <div class="mb-4 ms-3">
                                    <h5 class="mb-3">Enfants</h5>
